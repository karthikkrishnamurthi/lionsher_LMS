<%= stylesheet_link_tag 'courses_content'  %>
<%= stylesheet_link_tag 'question_bank' %>
<div id="div_container">
  <div id="div_mainpanel">
    <h1>Editing question_bank</h1>

    <% form_for(@question_bank) do |f| %>
      <%= f.error_messages %>

      <p>
        <%= f.label :name %><br />
        <%= f.text_field :name %>
      </p>
      <p>
        <%= f.submit "Update" %>
      </p>
    <% end %>

    <%= link_to 'Show', @question_bank %> |
    <%= link_to 'Back', question_banks_path %>
  </div>
  <div id="div_sidepanel">
    <%= render :partial => "/courses/logo" %>
  </div>
</div>