<h1>Listing question_statuses</h1>

<table>
  <tr>
    <th>Status value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @question_statuses.each do |question_status| %>
  <tr>
    <td><%= question_status.status_value %></td>
    <td><%= link_to 'Show', question_status %></td>
    <td><%= link_to 'Edit', edit_question_status_path(question_status) %></td>
    <td><%= link_to 'Destroy', question_status, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Question status', new_question_status_path %>
