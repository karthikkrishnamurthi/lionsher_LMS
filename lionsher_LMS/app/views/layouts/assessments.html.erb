<% if !(@assessment.nil? or @assessment.blank?) and !(@assessment.test_pattern.nil? or @assessment.test_pattern.blank?) and  @assessment.test_pattern.pattern_name == "AIEEE" and (params[:action] == "multi_page_test" or params[:action] == "single_page_test" or params[:action] == "feedback" or params[:action] == "instructions" or params[:action] == "about_test" or params[:action] == "test_specific_instructions") %>
  <%= render :partial => "/layouts/test_pattern_header" %>
  <%= yield  %>
  <%= render :partial => "/layouts/test_pattern_footer" %>
<% elsif !(@assessment.nil? or @assessment.blank?) and !(@assessment.test_pattern.nil? or @assessment.test_pattern.blank?) and  @assessment.test_pattern.pattern_name == "CMAT" and (params[:action] == "multi_page_test" or params[:action] == "single_page_test" or params[:action] == "feedback" or params[:action] == "instructions" or params[:action] == "about_test" or params[:action] == "test_specific_instructions" or params[:action] == "cmat_instructions") %>
  <%= render :partial => "/layouts/cmat_test_pattern_header" %>
  <%= yield  %>
  <%= render :partial => "/layouts/cmat_test_pattern_footer" %>
<% elsif !(@assessment.nil? or @assessment.blank?) and !(@assessment.test_pattern.nil? or @assessment.test_pattern.blank?) and  @assessment.test_pattern.pattern_name == "CAT" and (params[:action] == "multi_page_test" or params[:action] == "single_page_test" or params[:action] == "feedback") %>
  <%= yield  %>
<% elsif (params[:action] == "cat_review" or params[:action] == "instructions" or params[:action] == "about_test" or params[:action] == "test_specific_instructions" or params[:action] == "cat_instructions") %>
  <%= yield  %>
<% elsif !(@assessment.nil? or @assessment.blank?) and !(@assessment.test_pattern.nil? or @assessment.test_pattern.blank?) and @assessment.test_pattern.id == 1 and (params[:action] == "single_page_test" or params[:action] == "multi_page_test") %>
  <%= render :partial => "/layouts/assessment_header" %>
  <%= yield  %>
  <%= render :partial => "/layouts/courses_footer" %>
<% else %>
  <%= render :partial => "/layouts/courses_header" %>
  <%= yield  %>
  <%= render :partial => "/layouts/courses_footer" %>
<% end %>
